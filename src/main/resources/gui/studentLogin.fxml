<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GUI_Controller">
   <children>
      <TextField fx:id="loginStudentID" alignment="CENTER" layoutX="270.0" layoutY="140.0" prefHeight="25.0" prefWidth="183.0" promptText="Student ID" />
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="250.0" layoutY="280.0" minHeight="10.0" minWidth="10.0" mnemonicParsing="false" onAction="#onStudentMenuButtonClick" prefHeight="35.0" prefWidth="75.0" text="Login" />
      <Label alignment="CENTER" layoutX="250.0" layoutY="60.0" prefHeight="53.0" prefWidth="225.0" text="Please enter yout student ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="invalidLabelStudent" alignment="CENTER" contentDisplay="CENTER" layoutX="250.0" layoutY="220.0" prefHeight="25.0" prefWidth="213.0" />
      <Button alignment="CENTER" layoutX="400.0" layoutY="280.0" minHeight="10.0" minWidth="10.0" mnemonicParsing="false" onAction="#clearIdLogin" prefHeight="35.0" prefWidth="75.0" text="Clear" />
   </children>
</AnchorPane>
